<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>
  <%= f.label :details %>
  <%= f.text_area :details, class: 'form-control' %>
  <%= f.label :complete %>
  <%= f.check_box :complete, class: 'form-control' %>
  <%= f.submit  class: 'btn btn-primary' %>
<% end %>
